{
    "compilerOptions": {
        "baseUrl": "./",
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ],
        "esModuleInterop": true,
        "module": "nodenext",
        "paths": {
            "@badeball/cypress-cucumber-preprocessor/*": [
                "./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"
            ]
        }
    },
    "include": [
        // process only spec files
        "cypress/**/*.ts"
    ]
}